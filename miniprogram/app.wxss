/**app.wxss**/
.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
} 
view,img,ul,ol,li,h1,h2,h3,h4,h5,h6,button{
  margin: 0;
  padding: 0;
}
@font-face {
  font-family: 'iconfont';  /* project id 974591 */
  
}
.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
